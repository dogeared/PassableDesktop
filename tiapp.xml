    <description>Easy, secure password management</description>
    <!-- These values are edited/maintained by Titanium Developer -->
    <id>com.afitnerd.PassableDesktop</id>
    <name>PassableDesktop</name>
    <version>1.0</version>
    <publisher>Micah Silverman</publisher>
    <url>http://afitnerd.com/projects/passable</url>
    <icon>default_app_logo.png</icon>
    <copyright>2011 by Micah Silverman</copyright>
    <!-- Window Definition - these values can be edited -->
    <window>
        <id>initial</id>
        <title>PassableDesktop</title>
        <url>app://index.html</url>
